{
  "manifest_version": 2,

  "name": "Cloud Sweeper",
  "description": "This extension allows the user to clean their Google Drive",
  "version": "1.0",

   "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "quickstart.html",
	
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}